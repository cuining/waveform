{
  "name": "@zcool/waveform",
  "version": "1.0.4",
  "description": "draw audio waveforms",
  "main": "index.cjs.js",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "**"
  ],
  "sideEffects": false,
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c && yarn copy",
    "pretest": "tsc tests/test.tsx --noEmit --jsx preserve -m None -t ESNext --isolatedModules --esModuleInterop --skipLibCheck",
    "test": "jest",
    "test:dev": "jest --watch --no-coverage",
    "test:coverage:watch": "jest --watch",
    "copy": "copyfiles -f package.json readme.md LICENSE dist && json -I -f dist/package.json -e \"this.private=false; this.devDependencies=undefined; this.optionalDependencies=undefined; this.scripts=undefined; this.husky=undefined; this.prettier=undefined; this.jest=undefined; this['lint-staged']=undefined;\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuining/waveform.git"
  },
  "keywords": [
    "audio",
    "waveforms",
    "canvas"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zcued/waveform/issues"
  },
  "homepage": "https://github.com/zcued/waveform#readme",
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "@babel/plugin-transform-typescript": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@types/react": "^16.9.11",
    "copyfiles": "^2.1.1",
    "husky": "^3.1.0",
    "json": "^9.0.6",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.5",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-typescript2": "^0.25.2",
    "typescript": "^3.7.2"
  }
}
